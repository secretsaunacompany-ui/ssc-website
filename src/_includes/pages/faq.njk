    <!-- Schema.org - FAQPage -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "FAQPage",
        "mainEntity": [
            {% for item in faq.items %}
            {
                "@type": "Question",
                "name": "{{ item.question }}",
                "acceptedAnswer": {
                    "@type": "Answer",
                    "text": "{{ item.schemaAnswer }}"
                }
            }{% if not loop.last %},{% endif %}
            {% endfor %}
        ]
    }
    </script>

    <div id="faq" class="page page--has-bg">
        <div class="page-bg--fixed page-bg--opacity-10" style="background-image: url('https://res.cloudinary.com/dlhqdgmih/image/upload/q_auto,f_auto,w_1920/v1768271313/IMG_2819_bl3kb1.jpg');"></div>
        <section class="page-hero hero-overlay">
            <div class="hero-overlay__bg hero-overlay__bg--opacity-30" style="background-image: url('https://res.cloudinary.com/dlhqdgmih/image/upload/q_auto,f_auto,w_1920/v1768256595/IMG_3337_a48dbb.jpg');"></div>
            <div class="hero-overlay__content">
                <h1>Frequently Asked Questions</h1>
                <p>What you need to know about working with us</p>
            </div>
        </section>

        <section class="container">
            {% for item in faq.items %}
            <div class="faq-item fade-in">
                <h3 class="faq-question">{{ item.question }}</h3>
                <p class="faq-answer">{{ item.answer | safe }}</p>
            </div>
            {% endfor %}
        </section>
    </div>

